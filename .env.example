# PSA API Configuration
PSA_API_BASE_URL=https://api.psacard.com/publicapi/
PSA_API_TOKEN=your_psa_api_token_here
PSA_DAILY_LIMIT=100

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=psa_pregrader
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SSL=false

# Server Configuration
PORT=3001
NODE_ENV=development
JWT_SECRET=your_jwt_secret_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Cache Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# External APIs
EBAY_API_KEY=your_ebay_api_key
COMC_API_KEY=your_comc_api_key

# Security
CORS_ORIGIN=http://localhost:3000,http://localhost:8501
SESSION_SECRET=your_session_secret